{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "module": "commonjs",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "baseUrl": "src",
    "paths": {
      "@typed": ["lib/protocol/typed"],
      "@typed/schema": ["lib/protocol/typed/schema"],
      "@typed/schema/status": ["lib/protocol/typed/schema/status"],
      "@typed/schema/message": ["lib/protocol/typed/schema/message"],
      "@transaction": ["lib/transaction"],
      "@v1": ["lib/protocol/v1"],
      "@v1/schema": ["lib/protocol/v1/schema"],
      "@v1/schema/message": ["lib/protocol/v1/schema/message/index"],
    }
  },
  "files": [],
  "include": [],
  "references": [
    {
      "path": "./tsconfig.lib.json"
    },
    {
      "path": "./tsconfig.spec.json"
    }
  ]
}
